{
  "name": "@skadi/example",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Example application using Skadi packages",
  "engines": {
    "node": ">=22.17.1"
  },
  "scripts": {
    "dev": "tsx src/dynamo-demo.ts",
    "demo:dynamo": "tsx src/dynamo-demo.ts",
    "build": "tsup",
    "start": "node dist/dynamo-demo.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.848.0",
    "@aws-sdk/lib-dynamodb": "^3.848.0",
    "@skadi/dynamo": "workspace:*",
    "@skadi/utils": "workspace:*",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}